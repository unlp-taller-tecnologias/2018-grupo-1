{% extends 'base.html.twig' %}

{% block body %}
    <h1>Expedienteredes list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Observacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for expedienteRede in expedienteRedes %}
            <tr>
                <td><a href="{{ path('expedienteredes_show', { 'id': expedienteRede.id }) }}">{{ expedienteRede.id }}</a></td>
                <td>{{ expedienteRede.observacion }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('expedienteredes_show', { 'id': expedienteRede.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('expedienteredes_edit', { 'id': expedienteRede.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('expedienteredes_new') }}">Create a new expedienteRede</a>
        </li>
    </ul>
{% endblock %}

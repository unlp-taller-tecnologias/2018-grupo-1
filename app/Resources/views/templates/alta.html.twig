{% extends 'templates/layout.html.twig' %}

{# app/Resources/views/layout.html.twig #}
{% block head %}
<title>Alta de {{ parametro }}</title>
{% endblock %}
{% block body %}
<div class="text-center container col-md-6 mt-5">
  <h1>Alta de {{ parametro|capitalize }}</h1>
  <form class="form-signin">
    <div class="">
      <label for="descripcion" class="sr-only">Descripcion</label>
      <input type="text" id="descripcion" class="form-control" placeholder="Descripcion" required autofocus>
    </div>

      <button class="btn btn-lg bg-violet btn-block mt-3 active" type="submit">Aceptar</button>
    </form>
</div>
{% endblock %}

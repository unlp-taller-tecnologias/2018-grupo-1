{% extends 'templates/layout.html.twig' %}

{% block body %}
    <div class="text-left container col-md-6 mt-3">
        <h3>Modificar tipo de abogadx</h3>
        <div class="panel">
            <div class="panel-body"> 
                {{ form_start(edit_form) }}
                    {{ form_widget(edit_form) }}
                    <input class="btn btn-success btn-xs" type="submit" value="Editar" />
                {{ form_end(edit_form) }}
                <a class="btn btn-info btn-xs" href="{{ path('tipoabogado_index') }}">Volver</a>
                {{ form_start(delete_form) }}
                    <input class="btn btn-danger btn-xs" type="submit" value="Borrar">
                {{ form_end(delete_form) }}
            </div>
        </div>
    </div> 
{% endblock %}
